<!--pages/me/me.wxml-->
<view class="top">
   <image class="isimg" wx:if="{{geren.avatarUrl}}" src="{{geren.avatarUrl}}"></image>
   <button class="shouquan"  open-type="getUserInfo" bindgetuserinfo="getUserInfo" wx:else>登录</button>
   <text class="nicheng">{{geren.nickname}}</text>
   <text class="xiu" >{{geren.identity}}</text>
   <image class="isbg" src="https://onmylive.com/images/mebg.png"></image>
</view>
<view class="listall">
<view class="sessionlist" bindtap="dingdan">
   <view class="sessionleft">
   <image class="dd" src="https://onmylive.com/images/dd.png"></image>
   </view>
   <view class="name">全部订单</view>
   <view class="btn">
    <image src="https://onmylive.com/images/go2.png"></image>
   </view>
</view>
<view class="sessionlist" bindtap="zc">
   <view class="sessionleft">
   <image class="zc" src="https://onmylive.com/images/zc.png"></image>
   </view>
   <view class="name">我的资产</view>
   <view class="btn">
    <image src="https://onmylive.com/images/go2.png"></image>
   </view>
</view>

<view class="sessionlist" bindtap="sp">
   <view class="sessionleft">
   <image class="sp" src="https://onmylive.com/images/sp.png"></image>
   </view>
   <view class="name">我的水票</view>
   <view class="btn">
    <image src="https://onmylive.com/images/go2.png"></image>
   </view>
</view>

