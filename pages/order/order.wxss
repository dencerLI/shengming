checkbox{
  width: 34rpx; /* 背景的宽 */
  height: 42rpx; /* 背景的高 */

}
checkbox .wx-checkbox-input,radio .wx-radio-input {
  border-radius:4rpx; /* 圆角 */
  width: 34rpx; /* 背景的宽 */
  height: 34rpx; /* 背景的高 */
  background: #fff;
   box-sizing: border-box;
    margin-top: -8rpx;
}

