.select_box{
  width:220rpx;
  height:60rpx;
  float: right;
   margin-top: 30rpx;
  position: relative;
}
.select_box .select{
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
 
}
.select_box .select .select_text{
  font-size: 22rpx;
  color: #999999;
  line-height: 60rpx;
  flex: 1;
   text-align: center;
}
.select_box .select .select_img{
  width: 40rpx;
  height: 60rpx;
  display: block;
  transition:transform 0.3s;
}
.select_box .select .select_img_rotate{
  transform:rotate(90deg); 
}
.select_box .option_box{
  position: absolute;
  top: calc(100% - 1px);
  width: 180rpx;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  background: #fff;
  transition: height 0.3s;
  border-left:1px solid #efefef;
  border-right:1px solid #efefef;
  
}
.select_box .option_box .option{
  display: block;
  line-height: 60rpx; 
  font-size: 22rpx;
  /* border-top: 1px solid #efefef;
  border-bottom: 1px solid #efefef;
  padding: 10rpx; */
   text-align: center;
  color: #999999;

}


